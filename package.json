{
  "name": "treasure",
  "version": "1.0.0",
  "description": "Send GoogleAnalytics event when the reading through of the content",
  "author": "Tomoya Otsuka",
  "scripts": {
    "start": "npm-run-all -p 'build:* -- STG' copy imagemin",
    "dev": "npm-run-all -p 'watch:* -- DEV' server",
    "stg": "npm-run-all -p 'build:* -- STG' sitemap",
    "prd": "npm-run-all -p 'build:* -- PRD' sitemap",
    "build:html": "sh bin/pug.sh",
    "build:webpack": "sh bin/webpack.sh",
    "watch:html": "chokidar 'src/**/*.pug' --command 'npm run build:html'",
    "watch:webpack": "sh bin/webpack.sh -- --watch",
    "clear": "rimraf [ 'dist/**/*.{html,css,js}' ]",
    "copy": "cpx 'src/assets/**/*' dist",
    "imagemin": "glob \"src/images/**/*.{png,jpg,gif}\" | node node/imagemin.js -v -o dist",
    "server": "node node/server.js",
    "sitemap": "node node/sitemap.js"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-register": "^6.24.0",
    "browser-sync": "^2.18.8",
    "chokidar-cli": "^1.2.0",
    "cli-glob": "^0.1.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.27.3",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gsap": "^1.19.1",
    "imagemin": "^1.0.5",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "jquery": "^3.2.1",
    "json-merge": "^1.2.0",
    "json-minify": "^1.0.0",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss": "^5.2.16",
    "postcss-loader": "^1.3.3",
    "pug": "^2.0.0-beta6",
    "pug-cli": "github:jameskolce/pug-cli",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "slick-carousel": "^1.6.0",
    "style-loader": "^0.14.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1"
  }
}
